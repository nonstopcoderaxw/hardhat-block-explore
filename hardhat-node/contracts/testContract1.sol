pragma solidity ^0.8.17;

contract testContract1 {
    
    uint256 public var1;

    event ContractCreated(address _address);

    constructor() {
        var1 = 10000;

        emit ContractCreated(address(this));
    }

    function send(address payable to) external payable {
        to.transfer(msg.value);
    }    

}

